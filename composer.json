{
    "name": "sebastienheyd/boilerplate-email-editor",
    "description": "E-mail visual editor for sebastienheyd/boilerplate",
    "keywords": ["laravel", "boilerplate", "email", "mail", "e-mail", "editor"],
    "type": "library",
    "require": {
        "ext-libxml": "*",
        "ext-dom": "*",
        "sebastienheyd/boilerplate": "^7.0.1|@dev",
        "sebastienheyd/boilerplate-media-manager": "^7.0|@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "^3.0",
        "orchestra/testbench": "^5.0|^6.0",
        "doctrine/dbal": "^2.9",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastien HEYD",
            "email": "contact@sheyd.fr"
        }
    ],
    "autoload": {
        "psr-4": {
            "Sebastienheyd\\BoilerplateEmailEditor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sebastienheyd\\BoilerplateEmailEditor\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sebastienheyd\\BoilerplateEmailEditor\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
